{
    "source": {
        "include": ["backend", "Content/Blueprints"],
        "includePattern": ".+\\.(js|jsx|ts|tsx)$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "readme": "./README.md"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
} 